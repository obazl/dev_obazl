## set700/case720: demonstrate access to a sibling nslib within parent nslib

load(
    "@obazl_rules_ocaml//ocaml:rules.bzl",
    "ocaml_module",
    "ocaml_test",
)

ocaml_test(
    name = "test",
    main = "_Test"
)

ocaml_module(
    name   = "_Test",
    struct = "test.ml",
    opts = [
        "-w",
        "-27",  ## Error (warning 27): unused variable test_ctxt.
    ],
    deps = [
        "//namespaces/obazl/set700/case720/alpha:alpha",
        "@ounit2//:ounit2"
    ],
)
